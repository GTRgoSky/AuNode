
<body class="gray-bg">

    <div class="middle-box text-center loginscreen  animated fadeInDown">
        <div>
            <div style="margin-left: -35px;">

                <h1 class="logo-name">Sea</h1>

            </div>
            <h3>任务统一管理系统</h3>

            <form class="m-t" role="form" method="POST" action="/login">
                <div class="form-group">
                    <input type="text" class="form-control"  name='UserName' placeholder="用户名" required="">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" name="UserPassWord" placeholder="密码" required="">
                </div>
                <button type="submit" class="btn btn-primary block full-width m-b">登 录</button>


                <p class="text-muted text-center"><a href="/registPage">注册一个新账号</a>
                </p>

            </form>
        </div>
    </div>
<!--<script src="/dest/js/aa.js">-->
<script>
if(<%= errorMessage%>){
    setTimeout(function(){
        toastr.warning('登录信息有错误')
    },500)
    
}
</script>

</body>

